{
   int i,num;
    num=2;
    for(i=0;i<3;i++)
    {
        cout<<"num 變量為 "<<num<<end1;
        num++;
        {
            auto int num=1;
            cout<<"内置模快 num 變量為 "<<num<<end1;
            num++;
        }
    }
    return 0;
}
