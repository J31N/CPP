public class MainClass {
    public static void main(String[] args) {
     int i,num;
    num=2;
    for(i=0;i<3;i++)
    {
        System.out.printf("num 變量為 %d \n",num);
        num++;
        {
            auto int num=1;
            System.out.printf("内置模快 num 變量為 %d \n",num);
            num++;
        }
    }
   }
}
