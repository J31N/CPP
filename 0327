#include <stdio.h>
int main()
{
    int i,num;
    num=2;
    for(i=0;i<3;i++)
    {
        printf("num 變量為 %d \n",num);
        num++;
        {
            auto int num=1;
            printf("内置模快 num 變量為 %d \n",num);
            num++;
        }
    }
    return 0;
}
==============================================================================

