public class table99{

 public static void main(String[] args){
 
  for (int i = 1,j = 1; i < 10 ; i=(j==9)?(i+1):(i),j=(j==9)?(1):(j+1)){
   System.out.printf("%d*%d=%d\t",i,j,(i*j));
   if (j==9){
    System.out.println();
   }
  }
 }
}
