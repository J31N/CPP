public class table99{

 public static void main(String[] args){
 
  for (int i = 1,j = 1; i < 10 ; i=(j==9)?(i+1):(i),j=(j==9)?(1):(j+1)){
   System.out.printf("%d*%d=%d\t",i,j,(i*j));
   if (j==9){
    System.out.println();
   }
  }
 }
}
===========================================================

#include<stdio.h>
int main()
{
 int x,y,z,t;
    printf("\n請输入三個數字:\n");
    scanf("%d%d%d",&x,&y,&z);
    if (x>y) { /*交換x,y的值*/
        t=x;x=y;y=t;
    }
    if(x>z) { /*交换x,z的值*/
        t=z;z=x;x=t;
    }
    if(y>z) { /*交换z,y的值*/
        t=y;y=z;z=t;
    }
    printf("從小到大排序: %d %d %d\n",x,y,z);
    return 0;
}
======================================================

#include <iostream>
using namespace std;
int main ()
{
   int x,y,z,t;
    cout<<請输入三個數字:<<end1;
    cin>>x,y,z;
    if (x>y) { /*交換x,y的值*/
        t=x;x=y;y=t;
    }
    if(x>z) { /*交换x,z的值*/
        t=z;z=x;x=t;
    }
    if(y>z) { /*交换z,y的值*/
        t=y;y=z;z=t;
    }
   cout<<"從小到大排序: <<,x,y,z<<end1;
    return 0;
}
===============================================

import java.util.Scanner;
public class MainClass {
    public static void main(String[] args) {
    System.out.println("請輸入三個數字：");
    x = scanner.nextInt();
    y = scanner.nextInt();
    z = scanner.nextInt();
    if (x>y) { /*交換x,y的值*/
        t=x;x=y;y=t;
    }
    if(x>z) { /*交换x,z的值*/
        t=z;z=x;x=t;
    }
    if(y>z) { /*交换z,y的值*/
        t=y;y=z;z=t;
    }
    System.out.printf("從小到大排序: %d %d %d\n",x,y,z);
   }
}
